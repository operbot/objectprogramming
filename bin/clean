#!/usr/bin/env python3
# This file is placed in the Public Domain.


"cleanup"


import os


def popen(txt):
    "run command"
    for line in os.popen(txt).readlines():
        print(line)


popen("rm -fR build dist MANIFEST objectprogramming.egg-info")
popen("rm -fR bin/*~ *~")
popen("rm -fR op/*~ op/__pycache__")
popen("rm -fR opm/*~ opm/__pycache__")
popen("rm -fR .test test/__pycache__ test/*~")
